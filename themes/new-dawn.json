    },
    "tokenColors": [
        {
            "scope": "source",
            "settings": {
                "foreground": "#333333"
            }
        },
        {
            "name": "functions",
            "scope": [
                "meta.definition.method",
                "meta.function.shell",
                "entity.name.function"
            ],
            "settings": {
                "foreground": "#d69b12"
            }
        },
        {
            "name": "functions calls",
            "scope": [
                "meta.function-call",
                "meta.statement.command.shell",
                "meta.function-call.generic"
            ],
            "settings": {
                "foreground": "#d69b12",
                "fontStyle": "italic"
            }
        },
        {
            "name": "functions parameters",
            "scope": [
                "variable.parameter",
                "source variable.parameter",
                "variable.other.readwrite",
                "variable.other.object",
                "meta.method.declaration",
                "variable.other.assignment",
                "source.pip-requirements",
                "meta.function-call.arguments",
                "meta.attribute",
                "meta.fstring",
                "meta.object-literal.key"
            ],
            "settings": {
                "foreground": "#555555",
                "fontStyle": "italic"
            }
        },
        {
            "name": "object keys",
            "scope": "meta.object-literal.key",
            "settings": {
                "foreground": "#999999"
            }
        },
        {
            "scope": [
                "support.function.builtin",
                "support.function.magic"
            ],
            "settings": {
                "foreground": "#fcdc86",
                "fontStyle": "italic bold"
            }
        },
        {
            "scope": [
                "support.variable.property",
                "meta.type.parameters support.type"
            ],
            "settings": {
                "foreground": "#444444",
                "fontStyle": "italic bold"
            }
        },
        {
            "scope": [
                "markup.list.numbered.markdown"
            ],
            "settings": {
                "foreground": "#444444",
                "fontStyle": "italic"
            }
        },
        {
            "name": "properties",
            "scope": [
                "variable.object.property",
                "variable.other.property"
            ],
            "settings": {
                "foreground": "#444444"
            }
        },
        {
            "name": "primitive types",
            "scope": [
                "meta.type.parameters",
                "support.type.primitive.ts",
                "support.type.builtin",
                "punctuation.definition.decorator",
                "entity.name.function.decorator",
                "support.type"
            ],
            "settings": {
                "foreground": "#f6aac3",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "entity.name.type",
            "settings": {
                "foreground": "#f793b5"
            }
        },
        {
            "name": "operators",
            "scope": [
                "source keyword.operator",
                "punctuation.separator",
                "punctuation.section"
            ],
            "settings": {
                "foreground": "#A3C3C7"
            }
        },
        {
            "scope": [
                "punctuation.definition.template-expression.begin",
                "punctuation.definition.template-expression.end"
            ],
            "settings": {
                "foreground": "#d771589d"
            }
        },
        {
            "scope": [
                "constant.numeric",
                "constant.language"
            ],
            "settings": {
                "foreground": "#D19A66"
            }
        },
        {
            "name": "class",
            "scope": [
                "entity.name.type.class",
                "variable.other.constant.object"
            ],
            "settings": {
                "foreground": "#eb9744"
            }
        },
        {
            "name": "inhertited class",
            "scope": "entity.other.inherited-class",
            "settings": {
                "foreground": "#eb9744",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "storage.type",
                "keyword.control",
                "storage.modifier",
                "storage.type.property",
                "keyword.operator.logical.python",
                "keyword.operator.expression"
            ],
            "settings": {
                "foreground": "#B180D7"
            }
        },
        {
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#5a9b2b"
            }
        },
        {
            "scope": "string.template",
            "settings": {
                "foreground": "#5a9b2b",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "string.quoted.docstring.multi",
            "settings": {
                "foreground": "#6a954c"
            }
        },
        {
            "name": "none, null, undefined, imports, throw, exception",
            "scope": [
                "punctuation.accessor",
                "keyword.operator.logical",
                "meta.template.expression punctuation.definition",
                "keyword.control.trycatch",
                "keyword.operator.new",
                "entity.name.section",
                "entity.name.tag",
                "constant.language.python",
                "variable.other.readwrite.alias"
            ],
            "settings": {
                "foreground": "#d77158",
                "fontStyle": "italic"
            }
        },
        {
            "name": "this, super, self",
            "scope": [
                "variable.language",
                "support.type.property-name"
            ],
            "settings": {
                "foreground": "#c37a67"
            }
        },
        {
            "scope": [
                "support.type.exception"
            ],
            "settings": {
                "foreground": "#d77158",
                "fontStyle": "italic bold"
            }
        },
        {
            "scope": "log.error",
            "settings": {
                "foreground": "#d77158",
                "fontStyle": "italic bold"
            }
        },
        {
            "scope": "log.debug",
            "settings": {
                "foreground": "#6b7992",
                "fontStyle": "italic bold"
            }
        },
        {
            "scope": "log.warning",
            "settings": {
                "foreground": "#d69b12",
                "fontStyle": "italic bold"
            }
        },
        {
            "scope": "log.info",
            "settings": {
                "foreground": "#698ac9",
                "fontStyle": "italic bold"
            }
        },
        {
            "scope": [
                "comment",
                "markup.quote"
            ],
            "settings": {
                "foreground": "#6b7992",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "comment",
                "markup.quote"
            ],
            "settings": {
                "foreground": "#6b7992",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Hidden items",
            "scope": "markup.underline.link",
            "settings": {
                "foreground": "#555555"
            }
        },
        {
            "scope": [
                "variable.other.constant",
                "constant"
            ],
            "settings": {
                "foreground": "#698ac9"
            }
        }
    ]
}